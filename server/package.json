{
  "name": "hardwareexpress-backend",
  "version": "1.0.0",
  "description": "HardwareExpress Backend API Server",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "ai": "cd ai_service && .\\.venv\\Scripts\\python.exe -m uvicorn log_ai_service:app --host 127.0.0.1 --port 8001 --reload",    "dev:all": "concurrently \"npm run dev\" \"npm run ai\"",
    "build": "tsc",
    "start": "node dist/index.js",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.0",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.22.1",
    "jsonwebtoken": "^9.0.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.25",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^22.16.5",
    "concurrently": "^9.2.1",
    "tsx": "^4.7.0",
    "typescript": "^5.8.3"
  }
}
